    "@tanstack/react-table": "^8.20.5",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "firebase": "^10.13.2",
    "lucide-react": "^0.416.0",
    "openai": "^4.63.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
