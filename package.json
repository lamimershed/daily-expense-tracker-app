    "preview:test": "cross-env NODE_ENV=test vite preview --mode test --host",
    "format": "prettier --write src ",
    "format:check": "prettier --check src ",
    "prepare": "husky",
    "typecheck": "tsc -b"
  },
  "dependencies": {
